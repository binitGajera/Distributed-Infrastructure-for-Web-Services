    </xs:complexType>
  </xs:element>
  
    <xs:element name="Add">
    <xs:complexType>
      <xs:sequence>
        <xs:element name= "num1" type="xs:int"/>
        <xs:element name= "num2" type="xs:int"/>
      </xs:sequence>  
    </xs:complexType>
  </xs:element>
  
  <xs:element name="Multi">
    <xs:complexType>
      <xs:sequence>
        <xs:element name= "num1" type="xs:int"/>
        <xs:element name= "num2" type="xs:int"/>
      </xs:sequence>  
    </xs:complexType>
  </xs:element>
  
  <xs:element name="Sub">
    <xs:complexType>
